{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "javascriptreact",
    "json",
    "typescript",
    "typescriptreact"
  ],
  "cSpell.language": "en",
  "cSpell.ignorePaths": [
    "node_modules",
    "**/node_modules",
    "**/node_modules/**",
    "node_modules/**",
    ".git"
  ],
  "prettier.trailingComma": "all",
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,
  "prettier.printWidth": 80,
  "prettier.semi": true,
  "prettier.bracketSpacing": true,

  "javascript.suggest.autoImports": true,

  "typescript.preferences.importModuleSpecifier": "relative",

  "html.format.enable": true,
  "html.autoClosingTags": true,
  "html.filetypes": ["html", "handlebars", "hbs"],
  "cSpell.userWords": [
    "Formik",
    "Readmore",
    "copyfiles",
    "fastify",
    "lingui",
    "middlewares",
    "nestjs",
    "originalname",
    "persistor",
    "reqwest",
    "subreddit",
    "tmuxinator",
    "typegram",
    "youtube"
  ],
  "snippets.userSnippetsDirectory": "~/.config/nvim/snippets"
}
